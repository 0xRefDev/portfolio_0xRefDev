{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components/*": [
        "src/components/*"
      ],
      "@/layouts/*": [
        "src/layouts/*"
      ],
      "@/pages/*": [
        "src/pages/*"
      ],
      "@/interfaces/*": [
        "src/interfaces/*"
      ],
      "@/styles/*": [
        "src/styles/*"
      ],
      "@/assets/*": [
        "src/assets/*"
      ],
      "@/types/*": [
        "src/types/*"
      ],
      "@/utils/*": [
        "src/utils/*"
      ],
      "@/lib/*": [
        "src/lib/*"
      ],
      "@/icons/*": [
        "src/icons/*"
      ],
      "@/sections/*": [
        "src/sections/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}